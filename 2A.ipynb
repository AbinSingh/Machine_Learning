   "source": [
    "#Functions\n",
    "#Python functions are defined using the def keyword. For example:\n",
    "def sin(e):\n",
    "    if e > 0:\n",
    "        return 'positive'\n",
    "    elif e < 0:\n",
    "        return 'negative'\n",
    "    else:\n",
    "        return 'zero'\n",
    "\n",
    "for e in [-1, 0, 1]:\n",
    "    print(sin(e))\n",
    "# Prints \"negative\", \"zero\", \"positive\""
   ]
